{
  "sections": {
    "main": {
      "type": "main-article",
      "custom_css": [
        ".article-categries a {border: 1px solid #ccc; padding: 2px 6px; font-size: 85%; margin: 3px 12px 3px 0; display: inline-block;}",
        "#relatedblogs {padding-top: 30px;}",
        "#relatedblogs h3 {font-size: 150%; font-weight: normal; margin-bottom: 20px;}",
        "#relatedblogs ul {margin-inline-start: 0;}",
        "#relatedblogs .rb_contents {margin: 0;}",
        "#relatedblogs .rb_title {margin-bottom: 10px; font-size: 100%;}"
      ],
      "settings": {
        "show_image": true,
        "enable_parallax": false,
        "image_size": "medium",
        "content_width": "xs",
        "show_date": true,
        "show_category": true,
        "show_author": false,
        "show_share_buttons": true,
        "show_sticky_bar": false,
        "background": "#1c1c1c",
        "text_color": "#ffffff"
      }
    },
    "prev-next-blog-posts": {
      "type": "prev-next-blog-posts",
      "disabled": true,
      "settings": {
        "separate_section_with_border": false,
        "subheading": "",
        "title": "Read more",
        "show_category": false,
        "show_excerpt": true,
        "show_read_more": true,
        "background": "#1c1c1c",
        "text_color": "#ffffff"
      }
    }
  },
  "order": [
    "main",
    "prev-next-blog-posts"
  ]
}
